{
  "name": "wasm-cel",
  "version": "0.1.0",
  "description": "WebAssembly module for evaluating CEL (Common Expression Language) expressions in Node.js",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "GOOS=js GOARCH=wasm go build -ldflags '-s -w' -o main.wasm ./cmd/wasm",
    "build:copy-wasm-exec": "node scripts/copy-wasm-exec.js",
    "build:ts": "tsc",
    "build:all": "pnpm run build && pnpm run build:copy-wasm-exec && pnpm run build:ts",
    "prepublishOnly": "pnpm run build:all",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "keywords": [
    "cel",
    "common-expression-language",
    "wasm",
    "webassembly",
    "expression-evaluation"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/invakid404/wasm-cel.git"
  },
  "homepage": "https://github.com/invakid404/wasm-cel#readme",
  "bugs": {
    "url": "https://github.com/invakid404/wasm-cel/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": ["dist/**/*", "main.wasm", "wasm_exec.cjs", "README.md"],
  "devDependencies": {
    "@types/node": "^20.0.0",
    "jest": "^30.2.0",
    "typescript": "^5.0.0"
  }
}
